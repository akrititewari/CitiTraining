
public class Person {
	
	private int age;
	private String name;
	
	public Person() {
		
		age = 10; ;
		name = "random-name";	
	}
	
	public void display() {
		
		System.out.println("name:" + name + "\t" + "age:" + age);
	}
	
	public Person(int age, String name) {
		
		age = age;
		name = name;
	}
	
	public void setAge(int a) {
		age = a;
	}
}

