
public class Person {
	
	private int age;
	private String name;
	
	public void Person() {
		
		age = 10; ;
		name = "random-name";
		return 10;
		
	}
	
	public void display() {
		
		System.out.println("name:" + name + "\t" + "age:" + age);
	}
	
	public void Person(int a, String n) {
		
		age = a;
		name = n;
	}
}

